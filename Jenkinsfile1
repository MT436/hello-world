node{

stage 'MVN'
sh 'mvn clean package'

stage 'Docker'
sshPublisher(publishers: [sshPublisherDesc(configName: 'docker', sshRetry: [retries: 3, retryDelay: 10], transfers: [sshTransfer(cleanRemote: false, excludes: '', execCommand: '''docker build -t new:v1;
docker run -d --name example -p 8086:8080 new:v1 /bin/bash;''', execTimeout: 120000, flatten: false, makeEmptyDirs: false, noDefaultExcludes: false, patternSeparator: '[, ]+', remoteDirectory: './project4', remoteDirectorySDF: false, removePrefix: '', sourceFiles: '**/*.war')], usePromotionTimestamp: false, useWorkspaceInPromotion: false, verbose: false)])
}
